                    if ($s[0] == $csym || in_array($s[0], $wild)) {
                        $mpl = 1;
                        $tmpWin = $this->Paytable['SYM_' . $csym][1] * $betLine * $mpl * $bonusMpl;
                        if ($this->getCWin($k) < $tmpWin) {
