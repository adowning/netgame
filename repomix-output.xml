    private function getCWin($k) { return isset($this->cWins[$k]) ? $this->cWins[$k] : 0; }
    private function setCWin($k, $value) { $this->cWins[$k] = $value; }
    private function getLinesId() {
        return [
            0 => [
                [2, 2, 2, 2, 2],
