                    {
                        $postData['slotEvent'] = 'freespin';
                        $postData['action'] = 'freespin';
                    }
                    if( $postData['action'] == 'respin' ) 
                    {
                        $postData['slotEvent'] = 'respin';
                        $postData['action'] = 'spin';
                    }
