        $winType = $spinSettings[0];
        $spinWinLimit = $spinSettings[1];

        $totalWin = 0;
        $lineWins = [];
        $wild = ['0'];
        $scatter = '10';

        $reels = $this->GetReelStrips($winType, $slotEvent);
        $reelsTmp = $reels;

        for ($k = 0; $k < $lines; $k++) {
            $tmpStringWin = '';
            for ($j = 0; $j < count($this->SymbolGame); $j++) {
                $csym = $this->SymbolGame[$j];
                if ($csym == $scatter || !isset($this->Paytable['SYM_' . $csym])) {
                    continue;
