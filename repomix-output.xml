                    $s[2] = $reels['reel3'][$linesId[$k][2] - 1];
                    $s[3] = $reels['reel4'][$linesId[$k][3] - 1];
                    $s[4] = $reels['reel5'][$linesId[$k][4] - 1];
                    $p0 = $linesId[$k][0] - 1;
